{
  "name": "honkbeep-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "webpack serve -c webpack.client.config.js --port 3000",
    "start:production": "webpack serve -c webpack.client.config.js --env production --port 5000",
    "start:server": "webpack -c webpack.server.config.js --watch",
    "start:storybook": "start-storybook -p 6006",
    "build": "yarn build:client & yarn build:server",
    "build:client": "webpack -c webpack.client.config.js --env production --progress",
    "build:server": "webpack -c webpack.server.config.js --progress",
    "build:storybook": "build-storybook -o ./build/storybook",
    "deploy": "yarn build:client --no-devtool & yarn build:server --no-devtool --env production",
    "analyze": "webpack-bundle-analyzer ./build/client/stats.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:build": "yarn build & yarn build:storybook",
    "test:all": "yarn lint & yarn test & yarn test:build",
    "lint": "eslint . & prettier . -c",
    "lint:fix": "eslint . --fix & prettier . --write",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/core": "^7.13.15",
    "@koa/router": "^10.0.0",
    "@loadable/component": "^5.14.1",
    "@react-spring/web": "^9.1.1",
